package com.apama.cumulocity;

using com.softwareag.connectivity.ExtraFieldsDict;

/**
 * Sent in the case of any error.
 */
event Error {
	/** The error message. */
	string message;
}

/**
 * An operation to send to a device.
 *
 * Examples:
 * <pre>
 * Create an operation,
 * send Operation("","deviceId", "PENDING", {"c8y_Message":<any>{<any>"text":<any>"Hello Cumulocity device"}}) to Operation.CHANNEL;
 * <br>
 * You can update the status field.
 * send Operation("12345","deviceId", "EXECUTING", {}}) to Operation.CHANNEL;
 * <br>
 * </pre>
 */
@ExtraFieldsDict("params")
event Operation {
	/** Channel to send the events to. */
	constant string CHANNEL := "CumulocityIoT";
	/** Unique identifier for this Operation.
	 *
	 * Supply as empty if creating a new Operation */
	string id;
	/** Identifier of the ManagedObject to send the operation to. */
	string source;
	/** Status of the operation : one of SUCCESSFUL, FAILED, EXECUTING or PENDING. */
	string status;
	/** Any extra parameters available on the Operation. */
	dictionary<string, any> params;
}

/**
 * Request all Managed Objects to be sent.
 *
 * This can also be performed automatically on hostReady by setting transport property requestAllDevices (behaves as if
 * <pre>RequestAllAssets(-1, "cumulocity.devices")</pre> has been sent). On this being sent, a Managed Object event for every
 * Managed Object will be sent to the requested channel, followed by RequestAllDevicesComplete.
 * Send to CHANNEL (defined as a constant on this type).
 */
event RequestAllDevices {
	/**
	 * Channel to send RequestAllDevices events to.
	 */
	constant string CHANNEL := "C8YDeviceRequest";
	/**
	 * Unique identifier for this request.
	 * requestAllDevices configuration property uses requestId -1.
	 */
	integer requestId;
	/**
	 * Channel to send Asset, RequestAllAssetsComplete events to.
	 */
	string channel;
}

/**
 * Sent at the end of a RequestAllAssets stream of events.
 */
event RequestAllDevicesComplete {
	/**
	 * Unique identifier for this request.
	 * Events sent at startup if requestAllDevices is set uses requestId=-1.
	 */
	integer requestId;
	/**
	 * If not empty, indicates an error.
	 */
	string msg;
}

/**
 * Subscribe to measures.
 * The equivalent of sending SubscribeMeasurements("*") at hostReady can be achieved by setting configuration subscribeToAllMeasures to true.
 */
event SubscribeMeasurements {
	/**
	 * Wildcard value of assetId.
	 */
	constant string WILDCARD := "*";
	/**
	 * Channel to send these events to.
	 */
	constant string CHANNEL := "CumulocityIoT";
	/**
	 * The source to subscribe to, or WILDCARD for all assets.
	*/
	string assetId;
}

/**
 * Unsubscribe from measures.
 *
 * Subscriptions are ref-counted - multiple SubscribeMeasurements will require the same number of UnsubscribeMeasurements to unsubscribe.
 */
event UnsubscribeMeasurements {
	/**
	 * Wildcard value of assetId.
	 */
	constant string WILDCARD := "*";
	/**
	 * Channel to send these events to.
	 */
	constant string CHANNEL := "CumulocityIoT";
	/**
	 * The source to unsubscribe from, or WILDCARD for all assets.
	*/
	string assetId;
}

/**
 * Measurement value.
 */
@ExtraFieldsDict("params")
event MeasurementValue {
	/** Value from the sensor. */
	float value;
	/** Units the reading is in, for example mm, lux. */
	string unit;
	/** Any per-value extra fields. */
	dictionary<string, any> params;
}

/**
 * Measurement data.
 *
 * Sent by the transport when a device sends a new Measurement.
 * Requires SubscribeMeasurements or configuration property subscribeToAllMeasurements.
 * Sent to the cumulocity.measurements channel.
 *
 * Examples:
 * <pre>
 * Measurement("1001","c8y_LightMeasurement","12346081",1464359004.89,{"c8y_LightMeasurement":{"e":com.apama.cumulocity.MeasurementValue(108.1,"lux",{})}},{})
 * <br>
 * Measurement("1002","c8y_DistanceMeasurement","12346082",1464359005.396,{"c8y_DistanceMeasurement":{"distance":com.apama.cumulocity.MeasurementValue(344,"mm","","","",{})}},{})
 * </pre>
 *
 * Additionally, we can also create a new Measurement for a device.
 * To create a Measurement, send a Measurement event object to Measurement.CREATE_CHANNEL.
 *
 * Examples:
 * <pre>
 * send Measurement("","c8y_DistanceMeasurement","123",1464359005.396,{"distance":{"distance":MeasurementValue(3.44,"mm",{})}},{}) to Measurement.CREATE_CHANNEL;
 * </pre>
 */
@ExtraFieldsDict("params")
event Measurement {
	/** The channel to which <tt>Measurement</tt> events are sent from the transport.
	 */
	constant string CHANNEL:= "cumulocity.measurements";
	/** The channel to send a measurement event to create a new measurement object in Cumulocity.
	 */
	constant string CREATE_CHANNEL:= "CumulocityIoT";
	/** Unique identifier for this Measurement.
	 *
	 * Supply as empty if creating a new Measurement */
	string id;
	/** The type of the measurement. */
	string type;
	/** The unique identifier of the source of the measurement.
	 *  This should correspond to a ManagedObject. */
	string source;
	/** The time the measurement was taken, represented as the number of
	 * seconds since the epoch (1st January 1970). */
	float time;
	/** Measurement values mapped by fragment and series.
	 *
	 * Dictionary mapping from fragment name to a dictionary mapping from series name to
	 * MeasurementValue objects.
	 */
	dictionary<string, dictionary<string, MeasurementValue> > measurements;
	/** This dictionary is available to hold any other data
	 *  associated with the measurement. */
	dictionary<string, any> params;
}

/**
 * Event data.
 *
 * Sent by the transport when a device or other application creates an Event, or updates an Event.
 * Requires SubscribeMeasurements or configuration property subscribeToAllMeasurements.
 * Sent to the cumulocity.measurements channel.
 *
 * Examples:
 * <pre>
 * Event("21","c8y_EntranceEvent","12346082",1464364483.396,"Entrance event triggered.",{"distance":any(float,317)})
 * <br>
 * Event("22","com_cumulocity_events_TamperEvent","12345",1464364483.396,"Tamper sensor triggered",{"history":{}})
 * </pre>
 *
 * Additionally, we can also create a new Event or update an existing Event for a device.
 * To create/update an Event, send an Event event object to Event.CHANNEL.
 *
 * Examples:
 * <pre>
 * Create an Event,
 * send Event("","c8y_EntranceEvent","12346082",1464364483.396,"Entrance event triggered.",{"distance":any(float,317)}) to Event.CHANNEL;
 * <br>
 * You can update the text field.
 * send Event("22","c8y_EntranceEvent","12346082",1492676483.396,"Tamper sensor triggered",{}) to Event.CHANNEL;
 * </pre>
 */
@ExtraFieldsDict("params")
event Event {
	/** The channel for sending updates/ new Events to. */
	constant string CHANNEL :="CumulocityIoT";
	/** Unique identifier for this Event.
	 *
	 * Supply as empty if creating a new Event. */
	string id;
	/** The type of the Cumulocity event. */
	string type;
	/** Identifier of the source of the event. */
	string source;
	/** Timestamp of the event. */
	float time;
	/** Text or message of the Event. */
	string text;
