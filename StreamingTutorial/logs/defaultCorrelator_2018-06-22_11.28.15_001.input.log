package com.apama.cumulocity.internal;

using com.apama.cumulocity.RequestAllDevices;

/**
 * Disabled ForwardRequestAllDevices.
 */
event DisableBasicForwarder {
}

/**
 * Forwards RequestAllDevices to the transport.
 * Can be disabled by routing DisableBasicForwarder in the main context.
 */
monitor ForwardRequestAllDevices {
	constant string CUMULOCITY_CHANNEL := "CumulocityIoT";
	action onload {
		context ctx := context("Cumulocity.basicForwarder");
		on DisableBasicForwarder() {
			send DisableBasicForwarder() to ctx;
		}
		spawn basicForwarder to ctx;
	}
	action basicForwarder() {
		monitor.subscribe(RequestAllDevices.CHANNEL);
		on DisableBasicForwarder() {
			die;
		}
		RequestAllDevices rad;
		on all RequestAllDevices() as rad {
			send rad to CUMULOCITY_CHANNEL;
		}
		log "Apama Connectivity For Cumulocity IoT v10.2.0.2.329089 loaded" at INFO;
	}
}
